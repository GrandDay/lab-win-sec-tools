Thumbs.db
ehthumbs.db
Desktop.ini
$RECYCLE.BIN/
*.lnk

# PowerShell
*.ps1~
*.psd1~
*.psm1~

# Temporary files
*.tmp
*.temp
*.log
*.bak
*.swp
*~

